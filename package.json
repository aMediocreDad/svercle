		"eslint-visitor-keys": "^2.1.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aMediocreDad/svercle.git"
	},
	"keywords": [
		"superellipse",
		"ui",
		"squircle",
		"svelte"
	],
	"bugs": {
		"url": "https://github.com/aMediocreDad/svercle/issues"
	},
	"homepage": "https://github.com/aMediocreDad/svercle#readme"
